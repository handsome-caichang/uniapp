.detail {
  overflow: hidden;
  padding: 0 20rpx;
}
.detail .scroll-box {
    height: 800rpx;
}
.detail .towbtn {
    margin-top: 100rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-around;
            justify-content: space-around;
    padding: 0 100rpx;
}
.detail .towbtn .primary-btn {
      margin-right: 0;
      width: 204rpx;
      height: 104rpx;
      line-height: 104rpx;
      font-size: 38rpx;
      text-align: center;
      border-radius: 5px;
      color: #FFFFFF;
      background-color: #18C02C;
}
.detail .towbtn .error-btn {
      width: 204rpx;
      height: 104rpx;
      line-height: 104rpx;
      font-size: 38rpx;
      text-align: center;
      border-radius: 5px;
      color: #FFFFFF;
      background-color: #E7211A;
}
.detail .pop-container {
    background-color: #EBEBEB;
}
.detail .pop-container .top-box {
      text-align: center;
      height: 60rpx;
      line-height: 60rpx;
      font-size: 32rpx;
}
.detail .pop-container .uni-list .right {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
              flex: 1;
      margin-left: 30rpx;
}
.detail .btn-container {
    margin-top: 200rpx;
}
.detail .btn-container.contact .primary-btn {
      height: 100rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: center;
      -webkit-justify-content: center;
              justify-content: center;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      width: 40%;
}
.detail .btn-container.contact .primary-btn .uni-icons {
        margin-left: 10rpx;
}
.detail .primary-btn {
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    padding-left: 14px;
    padding-right: 14px;
    box-sizing: border-box;
    font-size: 18px;
    text-align: center;
    text-decoration: none;
    line-height: 2.55555556;
    border-radius: 5px;
    overflow: hidden;
    color: #FFFFFF;
    background-color: #18C02C;
}
.detail .s-container {
    margin-top: 30rpx;
}
.detail .s-container .scroll-view_H {
      white-space: nowrap;
      width: 100%;
}
.detail .s-container .scroll-view_H .img {
        display: inline-block;
        width: 160rpx;
        height: 202rpx;
        margin-right: 30rpx;
}
.detail .item {
    color: #303133;
}
.detail .item .btn {
      padding: 0 20rpx;
      border-radius: 10rpx;
      margin-left: 20rpx;
      color: #007aff;
      border: 1rpx solid #007aff;
}
.detail .item .title {
      font-size: 36rpx;
}
.detail .item .address {
      text-indent: 20rpx;
}
.detail .item .listbox {
      -webkit-flex-wrap: wrap;
              flex-wrap: wrap;
}
.detail .item .listbox .pro-type-item {
        margin-top: 20rpx;
        margin-right: 20rpx;
}

